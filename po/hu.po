#: libvnck/vnckprop.c:383 libvnck/vnckprop.c:407 libvnck/vnckprop.c:444
#: libvnck/vnckprop.c:468
#, c-format
msgid "Invalid value \"%s\" for --%s"
msgstr "A(z) --%s „%s” értéke érvénytelen"

#: libvnck/vnckprop.c:502 libvnck/vnckprop.c:521
#, c-format
msgid ""
"Conflicting options are present: screen %d should be interacted with, but --"
"%s has been used\n"
msgstr ""
"A kapcsolók ütköznek: a(z) %d. képernyővel kellene együttműködni, de a(z) --"
"%s került felhasználásra\n"

