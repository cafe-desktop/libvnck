msgstr ""
"Přesunout pohled na aktuální pracovní plochu na souřadnici Y označenou Y"

#: ../libvnck/vnckprop.c:190 ../libvnck/vnckprop.c:277
msgid "Y"
msgstr "Y"

#: ../libvnck/vnckprop.c:196
msgid "Minimize the window"
msgstr "Minimalizovat okno"

#: ../libvnck/vnckprop.c:198
msgid "Unminimize the window"
msgstr "Zrušit minimalizaci okna"

#: ../libvnck/vnckprop.c:200
msgid "Maximize the window"
msgstr "Maximalizovat okno"

#: ../libvnck/vnckprop.c:202
msgid "Unmaximize the window"
msgstr "Zrušit maximalizaci okna"

#: ../libvnck/vnckprop.c:204
msgid "Maximize horizontally the window"
